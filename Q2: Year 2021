from karel.stanfordkarel import *

"""
File: 2021.py
--------------------
Karel places 20 beepers, then 21 beepers, and 
ends facing East to the right of the 21 beepers.
"""
def main():
    for i in range(20):
        put_beeper()
    move()
    for i in range(21):
        put_beeper()
    move()

if __name__ == '__main__':
    run_karel_program()
